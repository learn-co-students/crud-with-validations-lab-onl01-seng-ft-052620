<h1>Welcome!</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Realease Year</th>
      <th>Released</th>
      <th>Genre</th>
      <th>Artist</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.released %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
      </tr>
    <% end %>
  </tbody>

</table>
