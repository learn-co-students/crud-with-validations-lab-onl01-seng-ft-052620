<%= form_for song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.label :genre %>
  <%= f.text_field :genre %><br>
  <%= f.label :release_year %>
  <%= f.text_field :release_year %><br>
   <%= f.label :released %>
  <%= f.check_box :released %><br>
<!-- 
  <%= f.label :released, "Released: " %>
  <%= f.radio_button :released, "true" %>
  <%= f.label :released_true, "true" %>
  <%= f.radio_button :released, "false" %>
  <%= f.label :released_true, "false" %><br>
-->
  <%= f.submit %>
<% end %>